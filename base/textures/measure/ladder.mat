{
	surfaceparm alphashadow
	surfaceparm trans

if $programs
	program defaultwall#MASK=0.5#MASKLT
	diffusemap "measure/ladder.tga"
else
	{
		map "measure/ladder.tga"
		alphaFunc GE128
		depthWrite
		rgbGen identity
	}
	{
		map $lightmap
		rgbGen identity
		blendFunc filter
		depthFunc equal
	}
endif
}
