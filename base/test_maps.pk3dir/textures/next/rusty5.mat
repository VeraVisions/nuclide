// Vera Visions Material
{
	surfaceParm metal

if $programs
	diffusemap "textures/next/rusty5.tga"
	{
		program defaultwall
		map $diffuse
		map $normalmap
	}
else
	{
		map $lightmap
		rgbGen identity
	}
	{
		map "textures/next/rusty5.tga"
		rgbGen identity
		blendFunc GL_DST_COLOR GL_ZERO
	}
endif
}
