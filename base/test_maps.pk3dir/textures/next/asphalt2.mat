// Vera Visions Material
{
if $programs
	diffusemap "textures/next/asphalt2.tga"
	normalmap "textures/next/asphalt2_normal.tga"
	{
		program defaultwall
		map $diffuse
		map $normalmap
	}
else
	{
		map $lightmap
		rgbGen identity
	}
	{
		map "textures/next/asphalt2.tga"
		rgbGen identity
		blendFunc GL_DST_COLOR GL_ZERO
	}
endif
}
